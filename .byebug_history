quit
def move_down(row, col)
  byebug
  while row >= 0
    row = row+1
    col = col
    pos_value = @grid[row][col]
    p "row_index: #{row}, col_index: #{col}"
    p "position_value: #{pos_value}"
    if pos_value != "O"
      break
    elsif row > @grid.length
      break
    else
    end
  end
end
quit
@grid.length
move_up(5,4)
move_down(0,4)
quit
